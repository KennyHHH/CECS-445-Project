@model IEnumerable<WebApplication1.Models.AdoptForm>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PetName)
        </th>
      
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>    
        <td>
            @Html.DisplayFor(modelItem => item.PetName)
        </td>
        <td>     
            @Html.ActionLink("Details", "Details", new { id=item.AdoptFormId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AdoptFormId })
        </td>
    </tr>
}

</table>
